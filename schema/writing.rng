<?xml version="1.0" encoding="UTF-8"?>
<!--
  
  Tree structure and modularization of content models 2012-10 by Mariana Paredes-Olea
  This schema is a representation and structural improvement of the Orlando SGML DTD writing.dtd in RELAX NG.  
  
  Initial content models transformaed 2010-07 by Syd Bauman, based on writing.dtd, as sent
  in e-mail "Re: Orlando conversion to XML: summary and meeting" from
  Susan Brown on 2010-06-21 20:19, and on
  biography-test_10-07-16-CAPS.rng, common_10-07-16.rnc, and
  events-test_10-07-16-CAPS.rnc "Re: drafts of
  schemas" from Mariana Paredes on 2010-07-16 12:37.
  Version 02: schema changes by Mariana Paredes-Olea:
  Added AREA element and attributes completed responsibility values, fixed hyphenated attributes and value names;
  missing hyphens and substrings, changed case of elements, attributes, and values
  names to reflect Orlando data (the current production system requires caps).
  
-->
<!--
  check list of changes to facilitate linked data
  check if other elements are shared with bio schema
-->

<!-- revised 2013-07-07
  revised 2013-10-02: relative URLs [MPO]
 -->

<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
  xmlns="http://relaxng.org/ns/structure/1.0"
  xmlns:xhtml="http://www.w3.org/1999/xhtml"
  datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">

  <include href="orlando_common_schema.rng">
    <start combine="choice">
      <ref name="Writing-element"/>
    </start>    
  </include>
  
  <define name="Writing-element" combine="choice">
    <element name="WRITING">
      <ref name="attlist.Writing"/>
      <ref name="header-element"/>
      <ref name="Div0-element"/>
    </element>
  </define>
  
  <!-- MAIN TREE -->
  
  <define name="chronProse-element" combine="choice">
    <element name="CHRONPROSE">
      <ref name="attlist.chronProse"/>
      <interleave>
        <optional>
          <ref name="chronProse-element"/>
        </optional>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-narrow-inclusion"/>
        <ref name="anyWri"/>
        <text/>
      </interleave>
    </element>
  </define>
  
  <define name="chronStruct-element" combine="choice">
    <element name="CHRONSTRUCT">
      <!-- Already documented in common schema. This definition includes writing semantic elements -->
      <zeroOrMore>
        <choice>
          <group>
            <choice>
              <ref name="dateRange-element"/>
              <ref name="dateStruct-element"/>
              <ref name="date-element"/>
            </choice>
            <ref name="chronProse-element"/>
            <optional>
              <ref name="shortProse-element"/>
            </optional>
          </group>
          <ref name="Div0-inclusion"/>
          <ref name="Div1-inclusion"/>
          <ref name="anyWri"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  
  <define name="Div0-element" combine="choice">
    <element name="DIV0">
      <ref name="attlist.Div0"/>      
      <interleave>
        <optional>
          <ref name="heading-element"/>
        </optional>
        <optional>
          <choice>
            <ref name="standard-element"/>
            <ref name="nonStandardDescriptor-element"/>
          </choice>        
        </optional>
        <optional>
          <ref name="authorSummary-element"/>
        </optional>      
        <optional>
          <ref name="worksCited-element"/>
        </optional> 
        <optional>
          <ref name="biogProse-element"/>
        </optional>
        <zeroOrMore>
          <ref name="Div1-element"/>
        </zeroOrMore>
        <ref name="Div0-inclusion"/><!-- inclusion from common schema: Div0: +( researchNote , title , bibCit , scholarNote , keywordClass , bibCits , lb) -->
      </interleave>
    </element>
  </define>
  
  <define name="Div1-element" combine="choice">
    <element name="DIV1">
      <ref name="attlist.Div1"/>
      <interleave>
        <optional>
          <ref name="heading-element"/>
        </optional>
        <zeroOrMore>
          <zeroOrMore>
            <ref name="textScope-element"/>
          </zeroOrMore>
          <oneOrMore>
            <ref name="Div2-element"/>
          </oneOrMore>
        </zeroOrMore>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-inclusion"/><!-- inclusion: Div1: +(dateStruct , date , dateRange , time , name , place , orgName , emph , foreign , soCalled , quote , sic , corr , rs , l , lg , topic , xref , xptr) -->
      </interleave>
    </element>
  </define>
  
  <define name="Div2-element" combine="choice">
    <element name="DIV2">
      <ref name="attlist.Div2"/>      
      <interleave>
        <optional>
          <ref name="heading-element"/>
        </optional>
        <zeroOrMore>
          <ref name="textScope-element"/>
        </zeroOrMore>
        <zeroOrMore>
          <choice>
            <ref name="production-element"/>
            <ref name="reception-element"/>
            <ref name="textualFeatures-element"/>
            <ref name="Div0-inclusion"/>
            <ref name="Div1-inclusion"/>
          </choice>
        </zeroOrMore>
        
      </interleave>      
    </element>
  </define>
  
  <define name="Div3-element" combine="choice">
    <element name="DIV3">
      <ref name="attlist.Div3"/>
      <interleave>
        <optional>
          <ref name="heading-element"/>
        </optional>
        <oneOrMore>
          <choice>
            <ref name="chronStruct-element"/>
            <ref name="dataStruct-element"/>
            <ref name="p-element"/>
          </choice>
        </oneOrMore>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-narrow-inclusion"/>
        <ref name="anyWri"/>
      </interleave>
    </element>
  </define>
  
  <define name="emph-element" combine="choice">
    <!-- mpo: defined in common schema, present definition includes tMotif -->
    <element name="EMPH">
      <ref name="ID-attribute"/>        
        <zeroOrMore>
          <choice>
            <text/>
            <ref name="tMotif-element"/>
            <ref name="anyWri"/>
            <ref name="Div0-inclusion"/>
            <ref name="Div1-inclusion"/>
          </choice>          
        </zeroOrMore>     
    </element>
  </define>
  
  <define name="foreign-element" combine="choice">
    <element name="FOREIGN">
      <optional><ref name="ID-attribute"/></optional>
      <optional><ref name="reg-attribute"/></optional>
      <optional>
        <ref name="lang-attribute"/>
      </optional>
      <!-- change to xml:lang ?-->
      <zeroOrMore>
        <choice>
          <text/>        
          <ref name="Div0-inclusion"/>
          <ref name="Div1-inclusion"/>
          <ref name="anyWri"/>
        </choice>          
      </zeroOrMore>
    </element>
  </define>
  
  <define name="l-element" combine="choice">
    <element name="L">
      <optional>
        <ref name="met-attribute"/>
      </optional>
      <optional>
        <ref name="real-attribute"/>
      </optional>
      <optional>
        <ref name="rhyme-attribute"/>
      </optional>
      <optional>
        <ref name="part-attribute"/>
      </optional>
      <interleave>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-narrow-inclusion"/>
        <ref name="anyWri"/>
        <text/>
      </interleave>
    </element>
  </define>
  
  <define name="name-element" combine="choice">
    <element name="NAME" >
      <ref name="entity-attributes"/>      
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="tCharacterName-element"/>
          <ref name="tCharacterTypeRole-element"/>
          <ref name="tThemeTopic-element"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  
  <define name="narrow-inclusion" combine="choice">
    <!--Inclusion from Div1 and Div0 except elements
        in DTD exclusion -(date, dateStruct, dateRange, keywordClass, name, orgName, place, 
        scholarNote, topic) -->
    <zeroOrMore>
      <choice>
        <ref name="time-element"/>
        <ref name="l-element"/>
        <ref name="lg-element"/>
      </choice>
    </zeroOrMore>
  </define>
  
  <define name="orgName-element" combine="choice">
    <!-- Documentation already included in common schema; this instance contains biography inclusions -->
    <element name="ORGNAME">
      <ref name="entity-attributes"/>      
      <optional>
        <ref name="orgType-attribute"/>
      </optional>
      <ref name="reg-attribute"/>
      <interleave>
        <text/>
        <optional>
          <ref name="narrow-inclusion"/>
        </optional>
        <zeroOrMore>
          <choice>
            <ref name="tCharacterTypeRole-element"/>
            <ref name="tCharacterName-element"/>
            <ref name="tThemeTopic-element"/>
            <ref name="job-element"/>
            <ref name="tMotif-element"/>
            <ref name="rRecognitionName-element"/>
          </choice>
        </zeroOrMore>       
        <!--<ref name="politics-inclusion"></ref> overlaps with culturalformation inclusion-->
      </interleave>
    </element>
  </define>
  
  <define name="p-element" combine="choice">
    <element name="P">
      <ref name="attlist.p"/>
      <interleave>
        <text/>
        <zeroOrMore>
          <choice>
            <ref name="Div0-inclusion"/>
            <ref name="Div1-inclusion"/>
            <ref name="genericRange-element"/>
            <ref name="ExtentOfOeuvre-element"/>
            <ref name="anyWri"/>
          </choice>
        </zeroOrMore>
      </interleave>
    </element>
  </define>
  
  <define name="quote-element" combine="choice">
    <element name="QUOTE">
      <ref name="attlist.quote"/>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="corrquoteitem-group"/>
            <text/>
            <ref name="anyWri"/>
            <ref name="ExtentOfOeuvre-element"/>            
          </choice>
        </zeroOrMore>
      </interleave>
    </element>
  </define>
  
  <define name="researchNote-element" combine="choice">
    <element name="RESEARCHNOTE">
      <ref name="ID-attribute"/>
      <interleave>
        <text/>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-narrow-inclusion"/>
        <ref name="anyWri"/>
      </interleave>
    </element>
  </define>
  
  <define name="shortProse-element" combine="choice">
    <element name="SHORTPROSE">
      <!-- LS to MP: No documentation for this even though it is mentioned many times in the documentation for other tags. -->
      <ref name="ID-attribute"/>
      <interleave>
        <oneOrMore>
          <ref name="p-element"/>
        </oneOrMore>
        <ref name="Div0-inclusion"/>
        <ref name="Div1-narrow-inclusion"/>
        <ref name="anyWri"/>
      </interleave>
    </element>
  </define>
  
